<Window x:Class="PASS.CashRegister.BillWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PASS.CashRegister"
        mc:Ignorable="d"
        Title="Účtenka - náhled" Width="400" Height="550" Closed="Window_Closed" >
    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="25"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        
        <Menu Grid.Row="0">
            <MenuItem Header="Tisk" FontSize="15" Click="btnPrint_Click"></MenuItem>
        </Menu>

        <WebBrowser x:Name="browser" Margin="3 3 3 3" Grid.Row="1"></WebBrowser>
        
    </Grid>
</Window>
